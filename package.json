{
	"name": "rebobinei-api",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"build": "npm run build:src && npm run build:seed",
		"build:src": "tsc -p tsconfig.json",
		"build:seed": "tsc -p tsconfig.seed.json",
		"vercel-build": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed",
		"start": "node dist/infra/http/server.js",
		"dev": "tsx watch --env-file .env src/infra/http/server.ts",
		"db:dev": "npx prisma migrate dev && npx prisma db seed",
		"db:prod": "npx prisma migrate deploy && npm run seed:prod",
		"db:reset": "npx prisma migrate reset --force",
		"seed:prod": "node dist/prisma/seed.js",
		"compose:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
		"compose:dev:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
		"compose:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d --build",
		"compose:prod:down": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@types/node": "^24.0.10",
		"prisma": "^6.11.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.1.0",
		"@fastify/jwt": "^9.1.0",
		"@fastify/swagger": "^9.6.1",
		"@fastify/swagger-ui": "^5.2.4",
		"@prisma/client": "^6.11.1",
		"axios": "^1.10.0",
		"bcryptjs": "^3.0.2",
		"fastify": "^5.4.0",
		"fastify-type-provider-zod": "^5.0.2",
		"zod": "^4.0.5"
	}
}
